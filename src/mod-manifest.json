{
    "apiVersion": "1.2",
    "version": "1.0",
    "name": "Box plot with line",
    "id": "js-dev-boxplot",
    "icon": "icon.svg",
    "properties": [
        {
            "name": "myProperty",
            "type": "string",
            "defaultValue": "myValue"
        },
        {
            "name": "orientation",
            "type": "string",
            "defaultValue": "vertical"
        },
        {
            "name": "line",
            "type": "string",
            "defaultValue": "none"
        }

    ],
    "dataViewDefinition": {
        "colorAxis": {
            "mode": "dual",
            "dropTarget": {
                "icon": "Color",
                "description": "Color by {0}"
            }
        },
        "axes": [
            {
                "name": "X",
                "mode": "categorical",
                "placement": "bottom"
            },
            {
                "name": "Y",
                "mode": "continuous",
                "placement": "left",
                "allowMultipleMeasures": false,
                "allowNonAggregatingMeasures": true,
                "automaticConfiguration": {
                    "expressionHeuristics": "none",
                    "priority": 1
                }
            }

        ]
    },
    "files": [
        "index.html",
        "main.css",
        "echarts.min.js",
        "echarts.dataTool.min.js",
        "main.js"
    ]
}